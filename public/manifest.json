{
    "manifest_version": 2,
    "name": "Yuyo",
    "author": "Antony Targett",
    "version": "0.0.1",
    "description": "Replace new tab screen with useful information.",
    "incognito": "split",
    "browser_action": {
        "default_icon": "images/Ant_Icon.png",
        "default_title": "GithubTab",
        "default_popup": "index.html?popup=true"
    },
    "icons": {
        "16": "images/ant_icon_16.png",
        "48": "images/ant_icon_48.png",
        "128": "images/ant_icon_128.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html?popup=false"
    },
    "permissions": ["storage", "activeTab"]
}
